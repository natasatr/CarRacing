        <Rectangle  Style="{StaticResource RectangleLinesSetup}" Canvas.Left="237" Canvas.Top="370"/>


        <Rectangle Tag="Auto" Width="55" Height="83" Fill="LightBlue" Canvas.Left="102" Canvas.Top="91"/>
